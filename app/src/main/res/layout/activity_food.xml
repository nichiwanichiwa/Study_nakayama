<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.nakayamanobuo.kadai1.food"
   >

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ホーム"
        android:id="@+id/home"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="非常食"
        android:id="@+id/button3"
        android:layout_alignBottom="@+id/home"
        android:layout_toEndOf="@+id/home" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="備蓄品"
        android:id="@+id/bichiku"
        android:layout_alignBottom="@+id/button3"
        android:layout_toEndOf="@+id/button3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="設定"
        android:id="@+id/setting"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/bichiku" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/retoruto_gohan"
        android:src="@drawable/retoruto_gohan"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="28dp" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/kandume"
        android:src="@drawable/kandume"
        android:layout_alignBottom="@+id/retoruto_gohan"
        android:layout_toEndOf="@+id/home" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/kanmen"
        android:src="@drawable/kanmen"
        android:layout_alignTop="@+id/kandume"
        android:layout_toStartOf="@+id/setting" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/kanpan"
        android:src="@drawable/kanpan"
        android:layout_above="@+id/textView22"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="主食"
        android:id="@+id/textView21"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="主菜"
        android:id="@+id/textView22"
        android:layout_below="@+id/retoruto_gohan"
        android:layout_alignParentStart="true"
        android:layout_marginTop="22dp" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/kandume_syusai"
        android:src="@drawable/kandume"
        android:layout_below="@+id/textView22"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/retoruto"
        android:src="@drawable/retoruto"
        android:layout_alignTop="@+id/kandume_syusai"
        android:layout_alignStart="@+id/kandume" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/furizu"
        android:src="@drawable/furizu_dorai"
        android:layout_alignTop="@+id/retoruto"
        android:layout_toStartOf="@+id/setting" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="その他"
        android:id="@+id/textView23"
        android:layout_below="@+id/kandume_syusai"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/mizu"
        android:src="@drawable/mizu"
        android:layout_below="@+id/textView23"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/pokari"
        android:src="@drawable/pokari_hunmatu"
        android:layout_below="@+id/textView23"
        android:layout_alignEnd="@+id/button3" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/karori"
        android:src="@drawable/karori_meito"
        android:layout_below="@+id/textView23"
        android:layout_toStartOf="@+id/okasi" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="90dp"
        android:id="@+id/okasi"
        android:src="@drawable/okasi"
        android:layout_alignTop="@+id/karori"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
